{
    "name": "vi-notify",
    "version": "1.0.0",
    "manifest_version": 3,
    "description": "A simple extension to notify students of Moodle. Neo-colab last dates.",
    "permissions": [
        "activeTab",
        "storage"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/icon16.png",
            "48": "icons/icon48.png",
            "128": "icons/icon128.png"
        }
    }
}